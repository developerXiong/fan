.container{
  width: 100%;
  min-height: 100%; 
  background-color:#F1F3F8;
}
.goBack{
  width: 70rpx;
  height: 70rpx;
  position: absolute;
   left: 4%;
  top: 65rpx;  
  z-index: 110;
  display: flex;
  justify-content: center;
  align-items: center;
}
.goBack image{
  width: 35rpx;
  height: 25rpx;
}
.header{
  width: 100%;
  height: 600rpx;
  position: relative;
}
.title{
  width: 100%;
  height:80rpx; 
  position: absolute;
  top: 54rpx;
  left: 0;
  font-size: 38rpx;
  color: white;
  z-index: 100;
  display: flex;
  justify-content: center;
  align-items: center;
}
.header_bg{
  width: 100%;
  height: 100%;
}
.header_bg image{
  width: 100%;
  height: 100%;
}
.header_cont{
  position: absolute;
  top: 140rpx;
  /* background: pink; */
  z-index: 100;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}
.h_c_top{
  width: 100%;
  height: 110rpx;
  display: flex;
  justify-content: center;
}
.month{
  height: 110rpx;
  width: 80%;
  white-space: nowrap;
  /* background: white; */
  line-height: 110rpx;
  color: white;
  font-size: 25rpx;
}
.month view{
  display:inline-block;
  margin-left: 50rpx;
  margin-right: 50rpx;
  padding-left: 4rpx;
  padding-right: 4rpx;
  height: 80rpx;
}
.month .scroll-view-item-normal{
  border-bottom: 2px solid transparent;

}
.month .scroll-view-item-select{
  border-bottom: 2px solid #00FFC6;
}
.prev,.next{
  width: 20rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.prev image{
  width: 20rpx;
  height: 30rpx;
}
.next image{
  width: 20rpx;
  height: 30rpx;
}
.h_c_bot{
  width: 92%;
  height: 88%;
  position: relative;
}
.h_c_bot1{
 background: rgba(255,255,255,.1);
 width: 100%;
 border-radius: 20rpx;
 position: absolute;
 top: 10%;
 height: 90%;
}
.h_c_bot2{
 background: rgba(255,255,255,.2);
 width: 92%;
 height: 95%;
 border-radius: 20rpx;
 position: absolute;
 left: 4%;
 top: 5%;
}
.h_c_bot3{
 width: 84%;
 height: 100%;
 border-radius: 20rpx;
 position: absolute;
 left: 8%;
 top: 0;
}
.h_c_bot_bg{
  width: 100%;
  height: 100%;
}
.h_c_bot_bg image{
  width: 100%;
  height:100%;
}
.h_c_bot_cont{
  position: absolute;
  top:0;
  left: 0;
  width: 100%;
  height:85%;
  padding-top: 15%;
}
.h_c_bot_cont .profit{
  display: flex;
  flex-wrap: wrap;
}
.h_c_bot_cont .profit .profit_num{
  font-size: 63rpx;
  color: #4837BC;
  width: 100%;
  text-align: center;
  font-weight: bold;
}
.h_c_bot_cont .profit .profit_words{
  font-size: 28rpx;
  font-weight: bold;
  color: #737B91;
  width: 100%;
  text-align: center;
}
.h_c_bot_cont .pro_under{
  display: flex;
  justify-content: space-around;
  flex-wrap: nowrap;
  width: 100%;
  margin-top: 50rpx;
}
.h_c_bot_cont .pro_under .public{
  width: 42%;
}
.h_c_bot_cont .pro_under .public .public_top{
  width: 100%;
  height: 60rpx;
  border-radius: 60rpx;
  border: 1px solid #B7BEC5;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: nowrap;
}
.h_c_bot_cont .pro_under .public .public_top .public_top_img{
  width: 25rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 10rpx;
}
.h_c_bot_cont .pro_under .public .public_top .public_top_img image{
  width: 100%;
  height: 20rpx;
}
.h_c_bot_cont .pro_under .public .public_top .public_num{
  font-size: 27rpx;
  color: #4837BC;
}
.h_c_bot_cont .pro_under .public .public_words{
  font-size: 28rpx;
  color: #8C9AA8;
  text-align: center;
  margin-top: 10rpx;
}


.prograss{
  width: 90%;
  height: 140rpx;
  background: #FFFFFF;
  box-shadow: 0 4px 6px 0 rgba(0,0,0,0.10);
  border-radius: 15rpx;
  margin-left: 5%;
  margin-top: 225rpx;
  overflow: hidden;
  display: flex;
  flex-wrap: nowrap;
}
.prograss .prograss_left{
  width: 65%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.prograss .prograss_left .pro_left_item{
  width: 80rpx;
  height: 100rpx;
   padding-left: 10rpx;
  padding-right: 10rpx; 
  box-shadow: 0 4px 6px 0 rgba(0,0,0,0.10);
  border-radius: 5px;
  font-size: 30rpx;
  font-weight: bold;
  color: #6C59CF;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 10rpx;
  margin-right: 10rpx;
}
.prograss .prograss_left .pro_left_item view{
  display: flex;
  justify-content: center;
}
.prograss .prograss_right{
  width: 35%;
  background: #6C59CF;
  box-shadow: 0 4px 6px 0 rgba(0,0,0,0.10);
  display: flex;
  flex-wrap: nowrap;
  justify-content: center;
  align-items: center;
}
.prograss .prograss_right .pro_right_words{
  font-size: 35rpx;
  color: white;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  /* font-weight: bold; */
}
.prograss .prograss_right .pro_right_words text{
  width: 100%;
  text-align: center;
}
.prograss .prograss_right .pro-right_img{
  width: 40rpx;
  height: 30rpx;
  display: flex;
}
.prograss .prograss_right .pro-right_img image{
  width: 100%;
  height: 100%;
}



.table{
  width: 94%;
  height: 417rpx;
  margin-left: 3%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  margin-top: 70rpx;
  padding-bottom: 60rpx;
  /* background: pink; */
}
.table .table_item{
  width:180rpx;
  height: 160rpx;
  padding-left: 15rpx;
  padding-right: 15rpx;
  padding-top: 20rpx;
  box-shadow: 0 4px 6px 0 rgba(0,0,0,0.10);
  border-radius: 5px;
  background: white;
}
.table .table_item .table_item_num{
  font-size: 36rpx;
  color: #737B91;
  white-space: nowrap;
  overflow: hidden;
  font-weight: bold;
}
.table .table_item .table_item_words{
  font-size: 26rpx;
  color: #737B91;
  opacity: .5;
  white-space: nowrap;
  overflow: hidden;
  margin-top: 18rpx;
}







.side{
  width: 100%;
  height: 100%; 
  background-color:transparent;
  position: absolute;
  left: -100%;
  top: 0;
  z-index: 10000;
  transition:.8s;
}
.side_show{
  width: 100%;
  height: 100%; 
  background-color:transparent;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 10000;
  transition: .5s;
}
.side_right{
  float: right;
  width: 250rpx;
  height: 100%;
   background: rgba(0,0,0,0); 
  transition: .5s;
}
.side_right_show{
  float: right;
  width: 250rpx;
  height: 100%;
  background: rgba(0,0,0,.4);
  transition: 2.5s;
}
.side_cont{
  width: 360rpx;
  height: 100%;
  padding-left:70rpx;
  padding-right: 70rpx; 
  background: white;
  position: absolute;
  left: 0;
  top: 0;
  /* display: flex;
  justify-content: center;
  flex-wrap: wrap; */
}
.side_cont .logo{
  width: 100%;
  height: 190rpx;
  margin-top: 114rpx;
  display: flex;
  justify-content: center;
}
.side_cont .logo image{
  width: 190rpx;
  height: 190rpx;
}
.side_cont .info{
  width: 100%;
  margin-top: 50rpx;
}
.side_cont .info .info_name{
  font-size: 40rpx;
  color: #4837BC; 
  font-weight: bold;  
}
.side_cont .info .info_email{
  font-size: 26rpx;
  color: #4837BC;
  font-weight: bold;
  margin-top: 10rpx;
}
.side_cont .handle{
  width: 100%;
  font-size: 32rpx;
  color: #4837BC;
  font-weight: bold;
  margin-top:80rpx;
}
.side_cont .handle .order{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.side_cont .handle .order .order_num{
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
  margin-left: 90rpx;
  color: white;
  background: #4837BC;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight:normal; 
}
.side_cont .handle .contact{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.side_back{
  width: 135rpx;
  height: 60rpx;
  /* background: pink; */
  position: absolute;
  bottom: 60rpx;
  left: 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.side_back .side_back_img{
  width: 60rpx;
  height: 60rpx;
}
.side_back .side_back_img image{
  width: 100%;
  height: 100%;
}
.side_back .side_back_words{
  opacity: 0.5;
  font-size: 16px;
  color: #4837BC;
  font-weight: bold;
}







