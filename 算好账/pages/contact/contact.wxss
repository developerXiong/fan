.container{
  min-height: 100%;
  width: 100%;
  position: relative;
  background: #FBFBFB;
}
.goBack{
  width: 70rpx;
  height: 70rpx;
  position: absolute;
   left: 5%;
  top:10rpx; 
  z-index: 10;
  display: flex;
  justify-content: center;
  align-items: center;
}
.goBack image{
  width: 55rpx;
  height: 55rpx;
}
.title{
  width: 100%;
  height:80rpx; 
  background: white;
  color: black;
  margin-top: 45rpx;
  font-size: 38rpx;
  z-index: 100;
  display: flex;
  justify-content: center;
  align-items: center;
}
.message{
  margin-top: 20rpx;
  height: 200rpx;
  background: white;
}
.message .mess_img{
  width: 170rpx;
  height: 100%;
  float: left;
  display: flex;
  justify-content: center;
  align-items: center;
}
.message .mess_img image{
  width: 110rpx;
  height:110rpx;
}
.message .mess_right{
  float: left;
  width: 578rpx;
  height: 100%;
  display: flex;
  /* flex-wrap: wrap; */
  flex-direction: column;
  justify-content: center;
  font-size: 28rpx;
}
.message .mess_right .phone{
  width: 100%;
}
.message .mess_right .phone .phone_words{
  color: #1D1D1D;

}
.message .mess_right .phone .phone_num{
  color: #006FB9;
}
.message .mess_right .time{
  width: 100%;
  color: #9B9B9B;
  font-size: 24rpx;
  margin-top: 10rpx;
}











