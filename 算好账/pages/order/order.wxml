<view class='container'> 
  <view class='goBack' bindtap='goBack'>
    <image src='../../image/goBack.png'></image>
  </view>
  <view class='cont_bg'>
    <image src='../../image/bg03@2x.png'></image> 
  </view>
  <view class='title'>我的订单</view>
  <view class='list'>
    <view class='list_title'>
      <view class='list_title_type'>类型</view>

      <view class='list_title_time'>时间</view>
    </view>
    <view class='list_item' wx:for="{{list}}">
      <view class='type'>{{item.type}}</view>
      <view class='time'>
        <text>{{item.time}}</text>
        <text>天</text>
      </view>
    </view>
    

    
  
  </view>

</view>