.container{
  min-height: 100%;
  width: 100%;
  position: relative;
}
.cont_bg{
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: -1;
}
.cont_bg image{
  width: 100%;
  height: 100%;
}
.goBack{
  width: 70rpx;
  height: 70rpx;
  position: absolute;
   left: 5%;
  top: 20rpx; 
  z-index: 10;
  display: flex;
  justify-content: center;
  align-items: center;
}
.goBack image{
  width: 35rpx;
  height: 25rpx;
}
.title{
  width: 100%;
  height:80rpx; 
  /* position: absolute; */
  margin-top: 45rpx;
  font-size: 38rpx;
  color: white;
  z-index: 100;
  display: flex;
  justify-content: center;
  align-items: center;
}
.list{
  width: 524rpx;
  margin: 42rpx;
  padding-left: 72rpx;
  padding-right: 72rpx;
  background: white;
  box-shadow: 0 4px 6px 0 rgba(0,0,0,0.10);
  border-radius: 5px;
  padding-bottom: 60rpx;
}
.list .list_title{
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  color: #4837BC;
  font-weight: bold;
}
.list .list_title .list_title_type{
  width: 15%;
  display: flex;
  justify-content: center;
}
.list .list_title .list_title_time{
  width: 15%;
  display: flex;
  justify-content: center;
}
.list .list_item{
  font-size: 24rpx;
  font-weight: bold;
  color: #4A4A4A;
  width: 100%;
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}








